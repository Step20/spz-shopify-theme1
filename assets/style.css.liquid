.card{
    display: inline-flex;  
    margin: .8vw;
  

   .a{  
        text-align:center;
        text-decoration: none; 
        .img{
            width:  100%;
        }
        .img:hover{
            opacity:.6;
            cursor: pointer;
        }
        .card-text{
            margin: .8vw;
            font-size: calc(.2vw + .6rem);
            .card-title{
                
                font-weight: 400;
                margin: auto;
