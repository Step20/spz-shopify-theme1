.container{
    height: 100vh;  
    width: 100%;
    overflow-x: hidden;
    display: table; 
    margin: auto; 
    justify-content: center;
    align-content: center;
}

.collection{
    height: 100%;  
    display: grid; 
    position: relative;
    grid-template-columns: auto auto auto auto;
    margin-top: 10%;
    margin: auto;
    justify-content: center;
    align-content: center;
    
   
}

.product-out{  
    height: 100%;  
    width: 100%;
    overflow-x: hidden;
    
    
    margin: auto; 

    align-content: center;
   .product-content{   
    justify-content: center;
    position: relative;
    margin-top: 6%;
        padding: 2vw;
        display: grid;  
        grid-template-columns: 25% 50% 25%;
         justify-space: between;
       
        .left{
            

            .title{
                font-size:  calc(1rem + .6vw);
            }
            .price{
                 margin-top: -.8vw;
                font-weight: 400;
                font-size: calc(1rem + .5vw);
            }

            .desc{
                
                margin-top: 2vw;
                .desc-title{
                    font-size: calc(1rem + .3vw);
                    font-weight: 400;
                }
                 .desc-text {
                    font-weight: 300;
                    margin-top: -1vw;
                    font-size: calc(.5rem + .5vw);
                    line-height: calc(1rem + .8vw);
                    
                }
            }
        }
        .mid{
            margin: auto;
            text-align: center;
           bottom: 0;
           display: block;
            .product-image{
                padding-bottom: 2vw;
                text-align: center;
                margin: auto;
                justify-self: center;
                width: 65%;
            
            }
            .product-image-thumbs{
                display: inline-flex;  
                gap: 2vw;
                margin: auto;
                justify-content: center; 
                padding: 0;
            }
            .thumb-img  {   
                align-self: center;
                text-align: center;
                display:flex;  
                padding: 10px;
                list-style: none; 
                width: 100%;
                height: 12vh;
                transition: all 0.3s ease;
            }
            .thumb-img:hover{
                cursor: pointer;
                opacity: .6;
                transition: all 0.3s ease;
                transform: translate(0px, -3px);
            }
            .small-view{
                display: none;
            }

        }
        .right{
           
            
            .label{
                font-weight: 500;
                font-size: 1vw;
                
            }

            .product-option{
                  
                width: 100%;
                {% comment %} input-box{
                    .radio{
                    background-color: black;
                    color: white;
                    
                    }
                } {% endcomment %}
                .input-out{
                    margin-top: .3vw;
                    justify-content: between;
                    text-align: center;
                    .input-field{
                        padding-top:1vw;
                        padding-bottom:1vw;                    
                        border: 1px solid black;

                        .input-box{
                            align-self: center;
                            align-items: center;
                            width: 100%;  
                            display: inline;
                            border:0;
                            text-align: center;
                            font-family: 'IBM Plex Mono', monospace;
                            
                        }
                        

                    .input-box label,
                    .input-box input {
                        margin: .5vw;
                        width: 100%;  
                        border: 0;
                        cursor: pointer;
                        text-align: center;
                    
                    }

                    .input-box:focus {
                        border: 0;
                    }

                    .input-box input[type="radio"] {
                        display: none;
                    }

                    .input-box label:hover {
                        background: #DDD;
                    }

                    .input-box input[type="radio"]:checked + label {
                        background: black;
                        color: white
                    }
                    }
                }
            }


            .quantity-out{
                margin-top: 1.5vw;
                .quantity-box{
                    margin-top: .5vw;
                    width: 100%;
                    border: 1px solid black;
                    .quantity-select{
                        margin: 0;
                        padding-top: 1vw;
                        padding-bottom: 1vw;
                     
                    text-align: center;
                        .minus{
                            width: 1vw;
                            border: 1px solid white;
                            background: white;
                            text-align: center;
                        }
                        .minus:hover{
                            cursor: pointer;
                        }
                        .plus{
                             width: 1vw;
                            border: 1px solid white;
                            background: white;
                            text-align: center;
                            
                        }
                        .plus:hover{
                            cursor: pointer;
                        }
                        .quantity-input{
                            text-align: center;
                            font-size: 1vw;
                            border: 1px solid white;
                            text-align: center;
                        }
                    }
                }
            }
            .btn-group{
                    margin-top: 1.5vw;
                    .btn-add-now{
                        font-size: calc(.5rem + .4vw);
                        margin-top: .5vw;
                        width: 100%;
                        display: block;
                        font-family: 'IBM Plex Mono', monospace;
                        padding-left: 2vw;
                        padding-right: 2vw;
                        padding-top: 1vw;
                        padding-bottom: 1vw;
                        color: black;
                        background-color: white;
                        border: 1px solid black;
                        transition: all 0.3s ease;
                    }
                    .btn-add-now:hover{
                        transition: all 0.3s ease;
                        /* transform: translate(0px, -3px); */
                        cursor: pointer;
                        color: white;
                        background-color: black;
                    
                    }
                    .btn-add{
                        font-size: calc(.5rem + .4vw);
                        margin-top: .5vw;
                        width: 100%;
                        display: block;
                        font-family: 'IBM Plex Mono', monospace;
                        padding-left: 2vw;
                        padding-right: 2vw;
                        padding-top: 1vw;
                        padding-bottom: 1vw;
                        color: white;
                        background-color: black;
                        transition: all 0.3s ease;                        border: 1px solid black;

                    }
                    .btn-add:hover{
                        transition: all 0.3s ease;
                        /* transform: translate(0px, -3px); */
                        cursor: pointer;
                        color: black;
                        background-color: white;
                        border: 1px solid black;
                    }
                }
            }
        
    }
}

