 .nav2{ 
      width: 100%;
       z-index: 9999999;
        position: fixed;
        top: 0;
        left: 0;
        font-size: calc(1rem + .4vw);
        font-weight: 400;
 
        .top{
            align-items: center;
            align-content: center;
            text-align: center;
            display: inline-flex;
            justify-content: space-between;
            top: 0;
          
            position: absolute;
            .text{
                justify-content: space-between;
                 text-decoration: none;
                 padding: 2vw;
                color: black;
                font-family: 'IBM Plex Mono', monospace;
            }
            .text:hover{ 
                cursor: pointer;
                opacity: .7;
             }
            .img{
                filter: invert(1);
                width: calc(2.2rem + 4%); 
                padding: .8vw;
            }
            .img:hover{ 
                cursor: pointer;
                opacity: .7;
             }
        }
        .bottom{
            width: 100%;
            text-align: center;
            display: inline-flex;
            justify-content: space-between;
            bottom: 0;
            position: fixed;
        
            .text{ 
                padding: 1vw;   
                text-decoration: none;
                color: black;
                font-family: 'IBM Plex Mono', monospace;
              
            }
            .text:hover{ 
                cursor: pointer;
                opacity: .7;
             }
        }
    } 



